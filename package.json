{
	"name": "frontend-github",
	"private": true,
	"version": "2.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"mock:oidc": "node samples/oidc-mock-server/server.mjs",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"postinstall": "[ \"$NODE_ENV\" = production ] && exit 0; paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.11.8",
		"@internationalized/date": "^3.7.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@oslojs/jwt": "^0.3.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/fhir": "^0.0.41",
		"@types/lodash-es": "^4.17.12",
		"@types/pg": "^8.11.11",
		"@unovis/svelte": "^1.5.0",
		"@unovis/ts": "^1.5.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.94",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.13",
		"drizzle-kit": "^0.30.3",
		"drizzle-orm": "^0.39.0",
		"formsnap": "^2.0.0",
		"lodash-es": "^4.17.21",
		"lucide-svelte": "^0.474.0",
		"oauth2-mock-server": "^7.2.0",
		"openid-client": "^6.1.7",
		"pg": "^8.13.1",
		"pino": "^9.6.0",
		"pino-pretty": "^13.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sveaflet": "^0.1.3",
		"svelte": "^5.19.3",
		"svelte-breadcrumbs": "^2.0.1",
		"svelte-check": "^4.1.4",
		"sveltekit-superforms": "^2.23.1",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"uuid": "^11.0.5",
		"vite": "^6.0.11",
		"vite-plugin-kit-routes": "^0.7.6",
		"vitest": "^3.0.4",
		"zod": "^3.24.1"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "0.15.5",
		"mode-watcher": "^0.5.1"
	},
	"volta": {
		"node": "20.18.2"
	},
	"packageManager": "pnpm@9.15.4",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=9.0.0"
	}
}
